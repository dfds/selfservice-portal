{
  "name": "@/dfds-ui/react-components/src",
  "description": "Shared icons within DFDS",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "version": "0.0.0-PLACEHOLDER",
  "sideEffects": false,
  "main": "./dist/cjs/index.js",
  "module": "./dist/index.js",
  "esnext": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:esm": "dfds-script babel ./src --env-name esm --out-dir ./dist --only-lib-files",
    "build:cjs": "dfds-script babel ./src --env-name cjs --out-dir ./dist/cjs --only-lib-files",
    "build:types": "tsc --project ./tsconfig.build.json && cpx \"./dist/**/*.d.ts\" \"./dist/cjs\" --clean",
    "build:prepare-package": "dfds-script prepare-package && prettier --write --loglevel silent ./dist/package.json",
    "clean": "rimraf dist",
    "tsc": "tsc --noEmit",
    "generate": "node scripts/generate",
    "prepublishOnly": "yarn run build:prepare-package && yarn run copy-svgs",
    "copy-svgs": "cpx \"./src/svgs/**/*\" \"./dist/svgs\" --clean"
  },
  "peerDependencies": {
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.5",
    "react": ">= 17.0.2",
    "react-dom": ">= 17.0.2"
  }
}
