{
  "name": "@/dfds-ui/react-components/src",
  "description": "Shared react components within DFDS",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "version": "0.0.0-PLACEHOLDER",
  "sideEffects": false,
  "main": "./dist/cjs/index.js",
  "module": "./dist/index.js",
  "esnext": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:esm": "dfds-script babel ./src --env-name esm --out-dir ./dist --only-lib-files",
    "build:cjs": "dfds-script babel ./src --env-name cjs --out-dir ./dist/cjs --only-lib-files",
    "build:types": "tsc --project ./tsconfig.build.json && cpx \"./dist/**/*.d.ts\" \"./dist/cjs\" --clean",
    "build:prepare-package": "dfds-script prepare-package && prettier --write --loglevel silent ./dist/package.json",
    "start": "yarn run build && yarn run watch",
    "watch": "chokidar \"./src/**/*\" -c \"yarn run build\"",
    "clean": "rimraf dist",
    "prepublishOnly": "yarn run build:prepare-package"
  },
  "peerDependencies": {
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.5",
    "react": ">= 17.0.2",
    "react-dom": ">= 17.0.2",
    "react-select": "^4.0.0"
  },
  "dependencies": {
    "@/dfds-ui/colors/src": "0.0.0-PLACEHOLDER",
    "@/dfds-ui/fonts": "0.0.0-PLACEHOLDER",
    "@/dfds-ui/hooks/src": "0.0.0-PLACEHOLDER",
    "@/dfds-ui/react-components/src": "0.0.0-PLACEHOLDER",
    "@/dfds-ui/spacing/src": "0.0.0-PLACEHOLDER",
    "@/dfds-ui/theme/src": "0.0.0-PLACEHOLDER",
    "@/dfds-ui/typography/src": "0.0.0-PLACEHOLDER",
    "@kunukn/react-collapse": "^2.2.9",
    "@mui/base": "^5.0.0-alpha.102",
    "@mui/material": "^5.10.10",
    "@mui/styles": "^5.10.10",
    "@mui/system": "^5.10.10",
    "@tippyjs/react": "^4.2.6",
    "classnames": "^2.3.2",
    "humps": "^2.0.1",
    "react-hook-intersection-observer": "^0.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "7.24.3",
    "@/dfds-ui/storybook-design": "0.0.0-PLACEHOLDER",
    "@types/humps": "2.0.2",
    "@types/uuid": "^8.3.4"
  }
}
